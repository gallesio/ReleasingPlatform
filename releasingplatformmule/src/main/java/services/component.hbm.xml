<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="services.Component" table="component">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="name">
			<column name="name" length="16" not-null="false" />
		</property>
		<property name="type">
			<column name="type" length="16" not-null="false" />
		</property>
		<property name="ref">
			<column name="ref" length="16" not-null="false" />
		</property>
	</class>
</hibernate-mapping>